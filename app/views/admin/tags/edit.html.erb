<!--タグ編集画面-->
<div class="container">
  <h3>タグ編集</h3>
  <%= render "layouts/errors", obj: @tag %>
  <div class="row mt-5">
    <%= form_with model:@tag, url: admin_tag_path(@tag), method: :patch do |f| %>
	    <%= f.label :tag_name, "タグ名" %>
	    <%= f.text_field :tag_name %>
	    <%= f.submit "変更を保存", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>